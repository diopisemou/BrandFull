<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BrandFull.Views.MsDetPage"
             xmlns:pages="clr-namespace:BrandFull.Views">

  <MasterDetailPage.Master>
    <pages:MsDetPageMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>

  <MasterDetailPage.Detail>

    <NavigationPage BarBackgroundColor="#dcb34a">
      <x:Arguments>
        <pages:MsDetPageDetail />
      </x:Arguments>
    </NavigationPage>

  </MasterDetailPage.Detail>

</MasterDetailPage>